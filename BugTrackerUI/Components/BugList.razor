<h3>All Bugs</h3>

@foreach (Bug bug in Bugs)
{
    <thead>
        <tr>
            <th>Id</th>
            <th>Title</th>
            <th>Description</th>
            <th>Priority</th>
        </tr>
    </thead>
}

@code {
    [Inject]
    public IBugService BugService { get; set; }
    public List<Bug> Bugs { get; set; }

        protected override void OnInitialized()
        {
            Bugs = BugService.GetBugs().OrderBy(x => x.Priority).ToList();
            base.OnInitialized();
        }
}
